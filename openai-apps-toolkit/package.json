{
  "name": "openai-mcp-server",
  "version": "1.0.0",
  "description": "OpenAI MCP Server Template for Cloudflare Workers",
  "main": "src/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:prod": "wrangler deploy --env production",
    "test": "./scripts/run_tests.sh",
    "test:local": "./scripts/run_tests.sh --local",
    "bundle": "./scripts/build_submission_bundle.sh",
    "secrets:setup": "echo 'Run: wrangler secret put OPENAI_API_KEY && wrangler secret put AGENT_ID && wrangler secret put VERIFICATION_TOKEN'",
    "logs": "wrangler tail",
    "logs:prod": "wrangler tail --env production"
  },
  "keywords": [
    "openai",
    "mcp",
    "cloudflare",
    "workers",
    "agent"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "wrangler": "^3.99.0"
  }
}
